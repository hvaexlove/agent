{
  "name": "agent-server",
  "version": "1.0.0",
  "description": "agent-server - open source server",
  "main": "main.ts",
  "scripts": {
    "start": "npm run serve",
    "serve": "nodemon dist/main.js",
    "build": "npm run build-ts",
    "build-tslint": "npm run tslint && npm run build-ts",
    "build-ts": "tsc",
    "watch": "npm run watch-ts",
    "watch-tslint": "npm run tslint && npm run watch-ts",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "pkg": "pkg ./dist/main.js --out-path=pkg/",
    "repkg": "npm run build && pkg ./dist/main.js --out-path=pkg/"
  },
  "dependencies": {
    "log4js": "^3.0.5",
    "pkg": "^4.3.4",
    "require-yml": "^1.3.1",
    "shelljs": "^0.8.2",
    "superagent": "^3.8.3",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "tslint": "^5.10.0",
    "typescript": "^3.0.1"
  },
  "author": "shaoshuai",
  "license": "ISC",
  "readmeFilename": "README.md"
}
